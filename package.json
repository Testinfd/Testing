{
  "name": "text-to-handwriting",
  "version": "1.0.0-rc.1",
  "description": "Modern tool that generates image that looks like handwriting from text.",
  "main": "js/app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "cypress run",
    "eslint": "eslint --fix \"js/**/**.mjs\"",
    "prettier": "prettier --write \"./index.html\" \"js/**/**.mjs\" \"css/**.css\"",
    "format": "npm run prettier",
    "cy": "cypress open",
    "dev": "vite",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Testinfd/Testing.git"
  },
  "keywords": [],
  "author": "Ripu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Testinfd/Testing/issues"
  },
  "homepage": "https://github.com/Testinfd/Testing#readme",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1"
  },
  "devDependencies": {
    "cypress": "^4.4.1",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "vite": "^5.0.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && eslint . --fix && npm run eslint"
    }
  }
}
